{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Admin Student Create Response",
  "type": "object",
  "required": [
    "success",
    "data"
  ],
  "properties": {
    "success": {
      "type": "boolean"
    },
    "data": {
      "type": "object",
      "required": [
        "id",
        "name"
      ],
      "properties": {
        "id": {
          "anyOf": [
            { "type": "integer" },
            { "type": "string", "minLength": 1 }
          ]
        },
        "name": {
          "type": "string",
          "minLength": 1
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
