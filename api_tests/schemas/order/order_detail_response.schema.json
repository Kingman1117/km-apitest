{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Order Detail Response",
  "type": "object",
  "required": [
    "success",
    "data"
  ],
  "properties": {
    "success": {
      "type": "boolean"
    },
    "data": {
      "type": "object",
      "anyOf": [
        {
          "required": [
            "orderInfo"
          ],
          "properties": {
            "orderInfo": {
              "type": "object",
              "anyOf": [
                {
                  "required": [
                    "itemList"
                  ],
                  "properties": {
                    "itemList": {
                      "type": "array",
                      "minItems": 1
                    }
                  }
                }
              ],
              "additionalProperties": true
            }
          }
        },
        {
          "required": [
            "itemList"
          ],
          "properties": {
            "itemList": {
              "type": "array",
              "minItems": 1
            }
          }
        },
        {
          "required": [
            "orderItems"
          ],
          "properties": {
            "orderItems": {
              "type": "array",
              "minItems": 1
            }
          }
        }
      ],
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
