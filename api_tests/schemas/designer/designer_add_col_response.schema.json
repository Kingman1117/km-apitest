{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Designer Add Column Response",
  "type": "object",
  "required": [
    "success"
  ],
  "properties": {
    "success": {
      "type": "boolean"
    },
    "id": {
      "anyOf": [
        { "type": "integer" },
        { "type": "string", "minLength": 1 }
      ]
    },
    "colId": {
      "anyOf": [
        { "type": "integer" },
        { "type": "string", "minLength": 1 }
      ]
    },
    "data": {
      "type": "object",
      "properties": {
        "id": {
          "anyOf": [
            { "type": "integer" },
            { "type": "string", "minLength": 1 }
          ]
        },
        "colId": {
          "anyOf": [
            { "type": "integer" },
            { "type": "string", "minLength": 1 }
          ]
        }
      },
      "additionalProperties": true
    }
  },
  "anyOf": [
    { "required": ["id"] },
    { "required": ["colId"] },
    { "required": ["data"], "properties": { "data": { "required": ["id"] } } },
    { "required": ["data"], "properties": { "data": { "required": ["colId"] } } }
  ],
  "additionalProperties": true
}
