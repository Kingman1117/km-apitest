{
  "caseId": "1150695810001062406",
  "name": "edupc访问文件模块，正常下载文件",
  "group": "edupc_func",
  "needsLogin": "none",
  "steps": [
    {
      "action": "goto",
      "url": "http://huaedu1-110.365hjy.com.faidev.cc/col.jsp?id=101",
      "description": "访问文件模块页面"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "click",
      "selector": ".d_download_icon",
      "fallbacks": ["a[download]", ".download-btn", "img[alt*='下载']", ".file-download"],
      "description": "点击文件下载图标"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "verify",
      "expect": "下载|文件|成功",
      "snapshot": true,
      "description": "验证文件正常下载"
    }
  ],
  "cleanup": null,
  "verify": "下载|文件"
}
