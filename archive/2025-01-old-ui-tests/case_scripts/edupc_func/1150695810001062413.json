{
  "caseId": "1150695810001062413",
  "name": "edupc正常登录学员，个人中心内容正常展示",
  "group": "edupc_func",
  "needsLogin": "none",
  "steps": [
    {
      "action": "goto",
      "url": "http://huaedu1-110.365hjy.com.faidev.cc/login.jsp",
      "description": "访问edupc登录页"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "iframe",
      "iframeSelector": "#J_memberLogin",
      "steps": [
        {
          "action": "fill",
          "selector": "input[name='memberAcct']",
          "value": "km7",
          "fallbacks": ["input[placeholder*='账号']", "input[type='text']"],
          "description": "填写登录账号km7"
        },
        {
          "action": "fill",
          "selector": "input[name='memberPswd']",
          "value": "123456",
          "fallbacks": ["input[placeholder*='密码']", "input[type='password']"],
          "description": "填写登录密码123456"
        },
        {
          "action": "click",
          "selector": "button[type='submit']",
          "fallbacks": ["input[type='submit']", "button:has-text('登录')", ".loginBtn"],
          "description": "点击登录按钮"
        }
      ],
      "description": "在#J_memberLogin iframe内完成登录"
    },
    {
      "action": "wait",
      "ms": 3000
    },
    {
      "action": "goto",
      "url": "http://huaedu1-110.365hjy.com.faidev.cc/mCenter.jsp",
      "description": "访问个人中心页面"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "click",
      "selector": "text=我的会员卡",
      "fallbacks": ["a:has-text('我的会员卡')", ".tab:has-text('我的会员卡')"],
      "description": "点击【我的会员卡】tab"
    },
    {
      "action": "wait",
      "ms": 1500
    },
    {
      "action": "click",
      "selector": "text=个人信息",
      "fallbacks": ["a:has-text('个人信息')", ".tab:has-text('个人信息')"],
      "description": "点击【个人信息】tab"
    },
    {
      "action": "wait",
      "ms": 1500
    },
    {
      "action": "verify",
      "expect": "个人信息|会员卡|个人中心|账号",
      "snapshot": true,
      "description": "验证个人中心内容正常展示"
    }
  ],
  "cleanup": null,
  "verify": "个人信息|会员卡|个人中心"
}
