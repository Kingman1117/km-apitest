{
  "caseId": "1150695810001062414",
  "name": "edupc正常注册学员以及退出登录",
  "group": "edupc_func",
  "needsLogin": "none",
  "steps": [
    {
      "action": "goto",
      "url": "http://huaedu1-110.365hjy.com.faidev.cc/login.jsp",
      "description": "访问edupc登录页"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "iframe",
      "iframeSelector": "#J_memberLogin",
      "steps": [
        {
          "action": "click",
          "selector": "text=注册账号",
          "fallbacks": ["a:has-text('注册账号')", ".register-link", "a:has-text('注册')"],
          "description": "点击【注册账号】链接"
        }
      ],
      "description": "在iframe内点击注册账号"
    },
    {
      "action": "wait",
      "ms": 1500
    },
    {
      "action": "iframe",
      "iframeSelector": "#J_memberLogin",
      "steps": [
        {
          "action": "fill",
          "selector": "input[name='memberAcct']",
          "value": "edupc{timestamp}",
          "fallbacks": ["input[placeholder*='账号']", "input[type='text']:first-of-type"],
          "description": "填写注册账号（edupc+时间戳，不超20字符）"
        },
        {
          "action": "fill",
          "selector": "input[name='memberPswd']",
          "value": "123456",
          "fallbacks": ["input[placeholder*='密码']", "input[type='password']:first-of-type"],
          "description": "填写密码"
        },
        {
          "action": "fill",
          "selector": "input[name='memberPswdRepeat']",
          "value": "123456",
          "fallbacks": ["input[placeholder*='确认密码']", "input[type='password']:last-of-type"],
          "description": "填写确认密码"
        },
        {
          "action": "click",
          "selector": "button[type='submit']",
          "fallbacks": ["input[type='submit']", "button:has-text('注册')", ".registerBtn"],
          "description": "点击注册按钮"
        }
      ],
      "description": "在iframe内填写注册信息并提交"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "fill",
      "selector": "input[placeholder*='姓名']",
      "value": "edupc{timestamp}",
      "fallbacks": ["input[placeholder*='请输入姓名']", ".member-name input"],
      "description": "完善信息弹窗填写姓名"
    },
    {
      "action": "wait",
      "ms": 500
    },
    {
      "action": "click",
      "selector": "button:has-text('提交')",
      "fallbacks": ["text=提交", "button:has-text('确定')", ".submit-btn"],
      "description": "点击提交完善信息"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "verify",
      "expect": "注册成功|个人中心|欢迎|首页",
      "snapshot": true,
      "description": "验证注册成功"
    },
    {
      "action": "evaluate",
      "script": "document.querySelector('.J_memberLoginLogout').click()",
      "description": "evaluate点击退出登录按钮(.J_memberLoginLogout)"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "verify",
      "expect": "登录|注册|首页",
      "snapshot": true,
      "description": "验证退出登录成功"
    }
  ],
  "cleanup": null,
  "verify": "注册成功|退出|登录"
}
