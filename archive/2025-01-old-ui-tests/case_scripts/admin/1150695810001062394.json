{
  "caseId": "1150695810001062394",
  "name": "管理后台正常添加电子书",
  "group": "admin",
  "needsLogin": "admin",
  "steps": [
    {
      "action": "goto",
      "url": "http://i.edu.fkw.com.faidev.cc/?__aacct=huaedu1#/curriculum/onlineCourse/ebookManage"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "click",
      "selector": "button:has-text('添加电子书')",
      "fallbacks": ["text=添加电子书", "button:has-text('添加')"],
      "description": "点击添加电子书按钮"
    },
    {
      "action": "wait",
      "ms": 1500
    },
    {
      "action": "fill",
      "selector": "input[placeholder*='书名']",
      "value": "电子书{timestamp}",
      "fallbacks": ["input[placeholder*='名称']", "input[placeholder*='请输入书名']", "input[placeholder*='请输入名称']"],
      "description": "输入电子书书名"
    },
    {
      "action": "selectResource",
      "type": "file",
      "description": "选择电子书文件"
    },
    {
      "action": "wait",
      "ms": 1000
    },
    {
      "action": "click",
      "selector": "button:has-text('保存')",
      "fallbacks": ["button:has-text('保 存')", ".footer button.el-button--primary"],
      "force": true,
      "description": "点击保存按钮"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "verify",
      "expect": "保存成功|操作成功|添加成功|电子书",
      "snapshot": true,
      "description": "验证正常添加电子书"
    }
  ],
  "cleanup": null,
  "verify": "保存成功|操作成功|添加成功"
}
