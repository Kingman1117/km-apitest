{
  "caseId": "1150695810001062383",
  "name": "H5实物商品订单",
  "group": "h5",
  "needsLogin": "student_h5",
  "steps": [
    {
      "action": "goto",
      "url": "http://huaedu1-110.m.365hjy.com.faidev.cc/webIndex.jsp#/tempPages/commodity/detail/detail?id=28"
    },
    {
      "action": "wait",
      "ms": 6000
    },
    {
      "action": "iframeClick",
      "iframeSelector": "#mainIframe",
      "selector": "text=/(购买|￥|立即购买)/",
      "description": "点击底部购买按钮，展开规格面板"
    },
    {
      "action": "wait",
      "ms": 2000
    },
    {
      "action": "iframeClick",
      "iframeSelector": "#mainIframe",
      "selector": "text=/(购买|立即购买|确定)/",
      "description": "规格面板内再次点击购买"
    },
    {
      "action": "wait",
      "ms": 6000
    },
    {
      "action": "iframe",
      "iframeSelector": "#mainIframe",
      "steps": [
        {
          "action": "fill",
          "selector": "input[placeholder*='提货人']",
          "value": "km7",
          "description": "填写提货人姓名"
        },
        {
          "action": "fill",
          "selector": "input[placeholder*='手机'], input[placeholder*='电话'], input[type='tel']",
          "value": "17620229111",
          "description": "填写提货人手机号"
        }
      ],
      "description": "在iframe内填写提货人信息"
    },
    {
      "action": "wait",
      "ms": 1000
    },
    {
      "action": "submitOrder",
      "platform": "h5",
      "useBalance": true,
      "iframeSelector": "#mainIframe",
      "description": "使用余额抵扣并提交订单"
    },
    {
      "action": "wait",
      "ms": 3000
    },
    {
      "action": "verify",
      "expect": "支付成功|订单详情|已购买|购买成功",
      "snapshot": true,
      "description": "验证H5实物商品订单提交成功"
    }
  ],
  "cleanup": {
    "type": "writeOff",
    "adminOrderUrl": "http://i.edu.fkw.com.faidev.cc/?__aacct=huaedu1#/order/index"
  },
  "verify": "支付成功|订单详情|已购买|购买成功"
}
