# ===================================
# 测试环境 URL 配置
# ===================================

# 管理后台 URL
ADMIN_BASE_URL=http://i.edu.fkw.com.faidev.cc
ADMIN_LOGIN_URL=http://i.fkw.com.faidev.cc/ajax/login_h.jsp?dogSrc=3

# H5端 URL
H5_BASE_URL=http://huaedu1-110.m.365hjy.com.faidev.cc

# EduPC端 URL
EDUPC_BASE_URL=http://huaedu1-110.365hjy.com.faidev.cc

# ===================================
# 测试账号配置
# ===================================

# 管理后台账号
KMSK_ADMIN_USERNAME=your_admin_username
KMSK_ADMIN_PASSWORD=your_admin_password

# 学生账号（H5端 + EduPC端）
KMSK_STUDENT_USERNAME=your_student_username
KMSK_STUDENT_PASSWORD=your_student_password

# ===================================
# TAPD 配置
# ===================================

# TAPD 基础配置
TAPD_ENABLED=true
TAPD_WORKSPACE_ID=your_workspace_id
TAPD_API_USER=your_api_user
TAPD_API_PASSWORD=your_api_password
TAPD_BASE_URL=https://api.tapd.cn

# TAPD 当前测试计划
TAPD_TEST_PLAN_ID=your_test_plan_id
TAPD_TEST_PLAN_NAME=your_test_plan_name
TAPD_TEST_PLAN_URL=https://www.tapd.cn/your_workspace/sparrow/test_plan/detail/your_plan_id

# TAPD 测试人员
TAPD_TESTER=自动化测试

# TAPD 回填间隔（秒）
TAPD_REPORT_INTERVAL_SECONDS=0.3

# ===================================
# 企业微信通知配置
# ===================================

# 企业微信 Webhook
WECOM_ENABLED=false
WECOM_WEBHOOK=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=your_webhook_key

# 通知策略
WECOM_NOTIFY_ON_SUCCESS=true
WECOM_NOTIFY_ON_FAILURE=true

# ===================================
# 测试执行配置
# ===================================

# API 限频延时（秒）
API_RATE_LIMIT_SECONDS=2

# 是否对所有用例延时（1=是，0=否，默认仅对标记用例延时）
API_RATE_LIMIT_ALL=0

# ===================================
# 测试数据配置（可选）
# ===================================

# 默认学生ID
DEFAULT_STU_ID=57711

# 默认 Union User ID
DEFAULT_UNION_USER_ID=57655

# 默认 wxapp_aid
DEFAULT_WXAPP_AID=3444128

# 默认 wxapp_id
DEFAULT_WXAPP_ID=110

# 默认 aid
DEFAULT_AID=31687084
